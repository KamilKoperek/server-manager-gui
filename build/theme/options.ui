<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object id="window" class="GtkWindow">
    <property name="title">GUI Server Manager</property>
    <property name="default-height">500</property>
    <property name="default-width">500</property>
    <child type="titlebar">
      <object class="GtkHeaderBar" id="header">
        <child type="start">
          <object class="GtkButton" id="refreshButton">
            <property name="label">Save</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object id="mainGrid" class="GtkGrid">
        <child>
          <object id="settings_optionsWindow" class="GtkScrolledWindow">
            <style>
              <class name="settings_optionsWindow"/>
            </style>
            <property name="propagate-natural-width">1</property>
            <property name="vexpand">1</property>
            <child>
              <object id="settings_optionsList" class="GtkGrid"/>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
